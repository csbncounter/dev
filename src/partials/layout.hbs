{{!
  navClass: function (navName) {
    if (navName === this.page) {
      return 'activePage';
    }
  }
}}

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="description" content="N-Counter 2016 is a national event where Battalion units will be able to come together and experience an amazing weekend of activities, community, and fellowship.">
<meta name="keywords" content="N-Counter 2016, Register, Weekend Event, CSB Ministries, Get Excited, Get Inspired, Get Involved,">
  <meta name="author" content="Towson Web Team">
    
